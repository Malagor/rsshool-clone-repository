{
    "env": {
        "browser": true,
        "es6": true,
        "es2020": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "babel-eslint",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "guard-for-in": 0,
        "no-restricted-syntax": 0,
        "import/prefer-default-export": "off",
        "no-param-reassign": 0,
        "no-underscore-dangle": 0
    }
}
